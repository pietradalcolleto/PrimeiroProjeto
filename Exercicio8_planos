<!DOCTYPE html>

<html>

<head> <meta charset="utf-8"></head>

<body>

<script>

var plano = prompt("Digite seu plano - A, B ou C");
var salario = prompt("Digite seu salário");

if (plano = "A"){
document.write("Seu salário ficará: R$ " + salario*1.10);
}else if (plano = "B"){
document.write("Seu salário ficará: R$ " + salario*1.15);
}else if (plano = "C"){
document.write("Seu salario ficará: R$ " + salario*1.20);
}





</script>

</body>
</html>
